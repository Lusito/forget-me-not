{
  "compilerOptions": {
    "target": "ES2015",
    "module": "CommonJS",
    "lib": ["es2015", "dom", "dom.iterable"],
    "jsx": "react",
    "jsxFactory": "h",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "noEmit": true
  },
  "exclude": ["dist"]
}
